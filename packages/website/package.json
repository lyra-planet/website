{
	"name": "@ioclub/website",
	"version": "0.1.0",
	"sideEffects": false,
	"scripts": {
		"init-db": "run-s init-db:*",
		"init-db:generate": "prisma generate",
		"init-db:push": "prisma db push",
		"init-db:seed": "prisma db seed",
		"serve": "remix-serve build",
		"build": "run-s build:*",
		"build:uno": "unocss \"app/**/*.{tsx,jsx,mdx}\" -c unocss.config.ts -o app/styles/uno.css -m",
		"build:remix": "remix build",
		"dev": "run-p dev:*",
		"dev:uno": "unocss \"app/**/*.{tsx,jsx,mdx}\" -c unocss.config.ts -o app/styles/uno.css -w",
		"dev:remix": "remix dev"
	},
	"prisma": {
		"seed": "node --require esbuild-register prisma/seed.ts"
	},
	"dependencies": {
		"@babylonjs/core": "^5.27.0",
		"@babylonjs/gui": "^5.27.0",
		"@prisma/client": "4.4.0",
		"@remix-run/react": "^1.7.2",
		"@remix-run/serve": "^1.7.2",
		"@remix-run/server-runtime": "^1.7.2",
		"@types/marked": "^4.0.7",
		"@unocss/preset-mini": "0.45.29",
		"@unocss/reset": "0.45.29",
		"faunadb": "^4.7.0",
		"front-matter": "^4.0.2",
		"highlight.js": "^11.6.0",
		"katex": "^0.16.2",
		"marked": "^4.1.1",
		"mdx-bundler": "^9.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-katex": "^3.0.1",
		"react-uuid": "^1.0.3",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-highlight": "^5.0.2",
		"rehype-slug": "^5.0.1",
		"rehype-toc": "^3.0.2",
		"remark-gfm": "^3.0.1",
		"remix-utils": "^3.3.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@iconify/json": "^2.1.119",
		"@julr/unocss-preset-heropatterns": "^0.0.2",
		"@kidonng/daisyui": "^2.31.0",
		"@remix-run/dev": "1.7.2",
		"@types/highlight.js": "^10.1.0",
		"@types/uuid": "^8.3.4",
		"@unocss/cli": "0.45.29",
		"@unocss/preset-attributify": "0.45.29",
		"@unocss/preset-rem-to-px": "0.45.29",
		"@unocss/transformer-directives": "0.45.29",
		"esbuild-register": "^3.3.3",
		"prisma": "4.4.0",
		"prop-types": "^15.8.1",
		"typescript": "^4.8.4",
		"unocss": "0.45.29",
		"unocss-preset-daisy": "^1.2.0",
		"unocss-preset-scalpel": "^1.2.5",
		"unocss-preset-scrollbar": "^0.2.1",
		"vite": "^3.1.6"
	},
	"engines": {
		"node": ">=16"
	},
	"peerDependencies": {
		"prop-types": "^15.8.1",
		"vite": "^3.1.6"
	}
}
